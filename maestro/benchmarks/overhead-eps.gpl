set size 0.7
set key left
set term postscript eps
set data style linespoints
set xlabel "Number of processors"
set ylabel "Speedup"
plot \
 'openWorld-disturbance-500-20000.txt' title '500x500, 20000 iterations',  \
 'openWorld-disturbance-2000-1000.txt' title '2000x2000, 1000 iterations',  \
 'openWorld-disturbance-5000-200.txt' title '5000x5000, 200 iterations',  \
 'openWorld-disturbance-10000-50.txt' title '10000x10000, 50 iterations',  \
 x title 'linear'
