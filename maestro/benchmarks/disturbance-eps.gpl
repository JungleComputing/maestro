set size 0.8
set key bottom right
set term postscript eps
set data style linespoints
set xlabel "Number of nodes"
set ylabel "Execution time per frame"
set yrange [0:*]
plot \
 'plain-2000.table' using 1:4 title 'Homogeneous',  \
 'no-2000.table' using 1:4 title 'Disabled tasks',  \
 'slow-2000.table' using 1:4 title 'Slower tasks', \
 'oneslow-2000.table' using 1:4 title 'One slow task'
