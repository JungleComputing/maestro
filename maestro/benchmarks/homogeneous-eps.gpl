set size 0.7
set key left
set term postscript eps
set data style linespoints
set xlabel "Number of processors"
set ylabel "Execution time"
set yrange [0:*]
plot \
 'plain.table' title 'Homogeneous processors',  \
 'learnedplain.table' title 'Homogeneous processors, after initial learning',  \
 'onetask.table' title 'Entire flow as single task',  \
 'learnedonetask.table' title 'Entire flow as single task, after initial learning'
